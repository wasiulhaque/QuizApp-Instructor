<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Feedback">

    <TextView
        android:id="@+id/rcv_feed"
        android:layout_width="281dp"
        android:layout_height="46dp"
        android:text="@string/received_feedbacks"
        android:textColor="#43A037"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.054" />

    <Button
        android:id="@+id/feedback_1"
        android:layout_width="367dp"
        android:layout_height="102dp"
        android:text="Feedback 1   >"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rcv_feed"
        app:layout_constraintVertical_bias="0.04" />

    <Button
        android:id="@+id/feedback_2"
        android:layout_width="367dp"
        android:layout_height="102dp"
        android:background="#2699FB"
        android:text="Feedback 2    >"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rcv_feed"
        app:layout_constraintVertical_bias="0.26" />

    <Button
        android:id="@+id/feedback_3"
        android:layout_width="367dp"
        android:layout_height="102dp"
        android:background="#E8C8C8"
        android:text="Feedback 3    >"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rcv_feed"
        app:layout_constraintVertical_bias="0.481" />

    <Button
        android:id="@+id/feedback_4"
        android:layout_width="367dp"
        android:layout_height="102dp"
        android:background="#ECE9B7"
        android:text="Feedback 4    >"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rcv_feed"
        app:layout_constraintVertical_bias="0.705" />

    <Button
        android:id="@+id/back_button"
        android:layout_width="355dp"
        android:layout_height="62dp"
        android:background="#FB2626"
        android:text="@string/go_back"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rcv_feed"
        app:layout_constraintVertical_bias="0.902" />
</androidx.constraintlayout.widget.ConstraintLayout>